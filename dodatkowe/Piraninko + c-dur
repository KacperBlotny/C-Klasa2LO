*/





        // 200 - 1000 8klawiszy 150, 200, 275, 350, 525, 700, 875, 1050
        int x;

    while(x != 0)
    {

            cin >> x;

            if(x<=8)
            {



                if (x == 0) break;


                if(x == 1) Beep(150,200);
                if(x == 2) Beep(200,200);
                if(x == 3) Beep(250,200);
                if(x == 4) Beep(350,200);
                if(x == 5) Beep(450,200);
                if(x == 6) Beep(550,200);
                if(x == 7) Beep(650,200);
                if(x == 8) Beep(750,200);

            }



      else
      {     if(x==9)
            {
                for(int i=0; i<=1200;i = i+60)
                {
                Beep(400+i,700);
                }


            }

            else    cout << " nie ma takiego klawiszu\n";
      }



    }





    return 0;
}
