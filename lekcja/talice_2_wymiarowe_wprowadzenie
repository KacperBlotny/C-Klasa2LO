#include <iostream>
#include <cstdlib>
#include <ctime>
#include <algorithm>
#include <stdio.h>
#include <fstream>

using namespace std;

int main()
{
    int n = 10;
    int tab[n][n];

    for(int i=0; i<n ;i++)
    {
        for(int j=0; j<n; j++)
        {
            tab[i][j] = (i+1) * (j+1);
        }
    }

    for(int i=0; i<n ;i++)
    {
        for(int j=0; j<n; j++)
        {
            cout << tab[i][j] << "\t";
        }
        cout <<endl;
    }





    return 0;
}
