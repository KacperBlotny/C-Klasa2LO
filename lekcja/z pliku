#include <iostream>
#include <cstdlib>
#include <ctime>
#include <stdlib.h>
#include <fstream>

using namespace std;

int main()
{
    int x;
    ifstream in;
    in.open("liczby.txt");
    in >> x;
    for(int i=1; i<100 ;i++)
    {
    in >> x;
    if(x%7==0) cout << x <<endl;
    }

    in.close();


    system("PAUSE");
	return 0;
}
